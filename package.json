{
  "name": "frappe-erpnext-snippets",
  "displayName": "Frappe ERPNext Snippets",
  "description": "Ready-to-use Frappe Framework & ERPNext code snippets for Python, JS, and Jinja. ",
  "publisher": "FrappeERPnextSnippets",
  "version": "1.0.0",
  "icon": "assets/icon.png",
  "galleryBanner": {
    "color": "#2490ef",
    "theme": "light"
  },
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Snippets"
  ],
  "repository": {
  "type": "git",
  "url": "https://github.com/ehsanjavaid/Frappe-Erpnext-Snippets"
},
"license": "MIT",

  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "python",
        "path": "./snippets/frappe.json"
      },
      {
        "language": "html",
        "path": "./snippets/frappe.json"
      }
    ],
    "commands": [
      {
        "command": "frappe-erpnext-snippets-extension.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}