{
  "name": "frappe-erpnext-snippets",
  "displayName": "Frappe ERPNext Snippets",
  "description": "Ready-to-use Frappe Framework & ERPNext code snippets for Python, JS, and Jinja. ",
  "publisher": "FrappeERPnextSnippets",
  "version": "1.3.2",
  "icon": "assets/logo.png",
  "galleryBanner": {
    "color": "#2490ef",
    "theme": "light"
  },
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Snippets",
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ehsanjavaid/Frappe-Erpnext-Snippets"
  },
  "license": "MIT",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "python",
        "path": "./snippets/frappe.json"
      },
      {
        "language": "python",
        "path": "./snippets/erpnext.json"
      },
      {
        "language": "python",
        "path": "./snippets/bench.json"
      },
      {
        "language": "python",
        "path": "./snippets/jinja.json"
      }
    ],
    "commands": [
      {
        "command": "frappe-erpnext-snippets.helloWorld",
        "title": "Hello World",
        "category": "Frappe ERPNext Snippets"
      }
    ],
    "activationEvents": [
      "*"
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  }
}